<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>drLogin</servlet-name>
        <servlet-class>backend.drLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>patientregister</servlet-name>
        <servlet-class>backend.patientregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doctorregister</servlet-name>
        <servlet-class>backend.doctorregister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>patientlogin</servlet-name>
        <servlet-class>backend.patientlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>java</servlet-name>
        <servlet-class>payment.java</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>prescriptionstore</servlet-name>
        <servlet-class>backend.prescriptionstore</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Slotsstore</servlet-name>
        <servlet-class>backend.Slotsstore</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>drLogin</servlet-name>
        <url-pattern>/drLogin</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>patientregister</servlet-name>
        <url-pattern>/patientregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doctorregister</servlet-name>
        <url-pattern>/doctorregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>patientlogin</servlet-name>
        <url-pattern>/patientlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>java</servlet-name>
        <url-pattern>/java</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>prescriptionstore</servlet-name>
        <url-pattern>/prescriptionstore</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Slotsstore</servlet-name>
        <url-pattern>/Slotsstore</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <resource-ref>
        <res-ref-name>WebApplication1</res-ref-name>
        <res-type>javax.sql.ConnectionPoolDataSource</res-type>
        <res-auth>Container</res-auth>
        <res-sharing-scope>Shareable</res-sharing-scope>
    </resource-ref>
</web-app>
